package com.falcon.avisep.model;

import org.hibernate.annotations.Cache;
import org.hibernate.annotations.CacheConcurrencyStrategy;

import javax.persistence.*;
import javax.validation.constraints.*;
import java.io.Serializable;
import java.util.Objects;

/**
 * A VTeacher.
 */
@Entity
@Table(name = "v_teacher")
@Cache(usage = CacheConcurrencyStrategy.NONSTRICT_READ_WRITE)
public class VTeacher implements Serializable {

    private static final long serialVersionUID = 1L;

    @Id
    @GeneratedValue(strategy = GenerationType.SEQUENCE, generator = "sequenceGenerator")
    @SequenceGenerator(name = "sequenceGenerator")
    private Long vtId;

    @OneToOne(optional = false)
    @NotNull
    @JoinColumn(unique = true)
    private Form form;

    public Long getId() {
        return vtId;
    }

    public void setId(Long id) {
        this.vtId = id;
    }

    public Form getForm() {
        return form;
    }

    public VTeacher form(Form form) {
        this.form = form;
        return this;
    }

    public void setForm(Form form) {
        this.form = form;
    }

    @Override
    public boolean equals(Object o) {
        if (this == o) {
            return true;
        }
        if (o == null || getClass() != o.getClass()) {
            return false;
        }
        VTeacher vTeacher = (VTeacher) o;
        if (vTeacher.vtId == null || vtId == null) {
            return false;
        }
        return Objects.equals(vtId, vTeacher.vtId);
    }

    @Override
    public int hashCode() {
        return Objects.hashCode(vtId);
    }

    @Override
    public String toString() {
        return "VTeacher{" +
            "id=" + vtId +
            ", vtId='" + vtId + "'" +
            '}';
    }
}
